{
  "name": "nftaa-test",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "generate:types": "pnpm generate:defs && pnpm generate:meta",
    "generate:defs": "node_modules/.bin/polkadot-types-from-defs --package nftaa-test/interfaces --input ./shared/types/chain --endpoint ./edgeware.json",
    "generate:meta": "node_modules/.bin/polkadot-types-from-chain --package nftaa-test/interfaces --endpoint ./edgeware.json --output ./shared/types/chain",
    "lint": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@nuxt/ui": "^3.3.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@polkadot/api": "^16.4.3",
    "@polkadot/extension-dapp": "^0.61.5",
    "@polkadot/extension-inject": "^0.61.5",
    "@vueuse/nuxt": "13.6.0",
    "nuxt": "^4.0.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@polkadot/typegen": "^16.4.3"
  },
  "packageManager": "pnpm@10.14.0"
}